#Counting number
s="12223311"


#==================================================================
# Online Python compiler (interpreter) to run Python online.
# Write Python 3 code in this online editor and run it.
a=int(input("Enter any number : "))
for i in range(0,a):
    if(i%2==0):
        print("This is even Number ",i)
    else:
        print("This is Odd  Number ",i)
        

count = 1
for i in range(1,len(s)):
    if s[i]==s[i-1]:
        count+=1
    else:
        print((count,s[i-1]), end='')
        count=1
print((count,s[-1]));

#--------------------------------------
pack1=int(input("Enter the pack1 weight"))
pack1price=float(input("Enter the pack1 price"))
pack2=int(input("Enter the pack2 weight"))
pack2price2=float(input("Enter the pack2 price"))

pp1=pack1*pack1price
pp2=pack2*pack2price2

if( pp1>pp2):
    print("Pack 1 is Good : ",pp1);
else:
    print("Pack 2 is Good : ",pp2)

